{
    "exp_dir": "checkpoints/tutorial",
    "dataset": "cifar",
    "mode": "george",
    "seed": 0,
    "classification_config": {
        "model": "resnet50",
        "checkpoint_metric": "val_acc_rw",
        "optimizer_config": {
            "class_args": {
                "lr": 1e-5,
                "weight_decay": 1.0,
                "momentum": 0.9                
                
            },
            "class_name": "SGD"
        },
        "workers": 8,
        "criterion_config": {},
        "eval_only": false,
        "metric_types": [
            "loss",
            "acc"
        ],
        "num_epochs":250,
        "batch_size": 128,
        "scheduler_config": {
            "class_args": {
                "milestones": [
                    50,
                    75
                ]
            },
            "class_name": "MultiStepLR"
        },
        "gdro_config": {
            "checkpoint_metric": "val_subclass_rob_acc_rw",
            "optimizer_config": {
                "class_args": {}
            },
            "criterion_config":
             {"size_adjustment": 2},
            "uniform_group_sampling": true            
        },
        "dataset_config": {
            "subsample": false,
            "resize": false
        }
    },
    "reduction_config": {
        "model": "umap"
    },
    "cluster_config": {
        "model": "gmm",
        "metric_types": [
            "composition"
        ],
        "search_k": true
    }
}
